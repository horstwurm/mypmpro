                    <p class="bnr-para">Registrieren Sie sich noch heute<br> </p>
        					  <%= link_to new_user_registration_path do %>
        				    	<i class="btn btn-download">Registrieren</i>
        					  <% end %>
          					<%= link_to new_user_session_path do %>
          			      <i class="btn btn-more">Anmelden</i>
          					<% end %> 
